datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
    id            Int       @id @default(autoincrement())
    name          String    @unique
    description   String?
    stock         Int
    salePrice     Float
    purchasePrice Float
    createdAt     DateTime 
    udpatedAt     DateTime
    deletedAt     DateTime?
}
